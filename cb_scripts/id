# {
#   "name": "id",
#   "description": "Open the record page with the given recordId in the default org",
#   "usage": "dev id <recordId>"
# }
recordId=$1

if [ -z $recordId ]; then
    echo 'Record Id is required'
    exit 0
fi

sf mypim run script -o $DefaultAlias -f ~/DevOps/cb_js/openRecord.js $recordId
