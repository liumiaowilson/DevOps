# {
#   "name": "getActiveFormDefinition",
#   "description": "Get the active form definition related to a task record",
#   "usage": "dev getActiveFormDefinition <recordId>"
# }
recordId=$1

if [ -z $recordId ]; then
    echo 'Record Id is required'
    exit 0
fi

sf mypim run script -o $DefaultAlias -f ~/DevOps/cb_js/getActiveFormDefinition.js $recordId
