# {
#   "name": "query",
#   "description": "Run the given query in the default org",
#   "usage": "dev query <query>"
# }
query="$@"

if [ -z "$query" ];then
    query=`cat ~/.last_query`
fi

if [ -z "$query" ];then
    exit 0
fi

echo "$query" > ~/.last_query
sf data query -q "$query" -o $DefaultAlias
